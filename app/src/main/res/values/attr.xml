<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CirCleProgressBar">
        <attr name="circle_color" format="color"/>
        <attr name="circle_bg_color" format="color"/>
        <attr name="circle_width" format="dimension"/>
        <attr name="circle_bg_width" format="dimension"/>
        <attr name="text_color" format="color"/>
        <attr name="text_size" format="dimension"/>
        <attr name="total_angle" format="integer"/>
        <attr name="start_angle" format="integer"/>
        <attr name="current_progress" format="float"/>
        <attr name="max_progress" format="float"/>
    </declare-styleable>
    <declare-styleable name="LifeIndexItemLayout">
        <attr name="index_image" format="reference"/>
        <attr name="index_text" format="string"/>
        <attr name="index_desc" format="string"/>
    </declare-styleable>
    <declare-styleable name="FutureWeaItemLayout">
        <attr name="future_img" format="string"/>
        <attr name="future_date" format="string"/>
        <attr name="future_temp_min" format="integer"/>
        <attr name="future_temp_max" format="integer"/>
        <attr name="future_desc" format="string"/>
    </declare-styleable>
</resources>